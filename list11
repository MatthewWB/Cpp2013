#include <iostream>
#include <cstdio>
#include "plikWejsciowy.hpp"
#include "plikWyjsciowy.hpp"
#include "manipulatory.hpp"


int main() {

    std::cout << "****** ZADANIE PIERWSZE ******" << std::endl;
    obliczenia::zadaniePierwsze();

    std::cout << std::endl;
    std::string znakA, znakB, znakC;

    std::cin >> obliczenia::ignore(4) >> znakC;
    std::cout << znakC << std::endl;
    std::cin >> obliczenia::clearline >> znakA >> znakB;
    std::cout << znakA << obliczenia::comma << std::endl << znakB << std::endl;

    std::cout << "****** ZADANIE DRUGIE ******" << std::endl;
    obliczenia::zadanieDrugie();

    return 0;
}
